    
    <div class="container">
        <div class="jumbotron text-center">
             <h1>Dr <%= @user.firstname+' '+@user.lastname %></h1>
        </div>
    </div>

    <div class="container">
        <h3>Here is a list of your patients, Dr <%= +@user.lastname %> </h3>
            <% @user_patients.each do |p| %>
                <%= link_to p.firstname+' '+p.lastname, patient_path(p) ,{ :class => "patient_list"} %><br/>
            <%end%>
            
        <%= link_to 'New Patient', new_patient_path, { :class => "btn"} %>
    </div>
    